\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Example:
\end_layout

\end_inset


\begin_inset Separator latexpar
\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
Find 
\begin_inset Formula $\arg\max_{x}\sum-(x-2)^{2}$
\end_inset

 where 
\begin_inset Formula $x$
\end_inset

 is a 4-by-1 vector
\end_layout

\begin_layout LyX-Code
obj = @(x) sum((x-2).^2)
\end_layout

\begin_layout LyX-Code
options = optimset('Display','iter','TolX',1e-8);
\end_layout

\begin_layout LyX-Code
[x fval] = fminsearch(obj,randn(4,1),options);
\end_layout

\begin_layout Itemize
optimize = minimize, so remember to change sign if looking for max
\end_layout

\end_deeper
\begin_layout Standard
\begin_inset Separator parbreak
\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Function Handles:
\end_layout

\end_inset


\begin_inset Separator latexpar
\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
f = @(x,y) x^2+y^2; f(2,3);
\end_layout

\begin_layout Itemize
f 1 = @(x,y) [f(x,y);f(x,y)+1];
\end_layout

\begin_layout Itemize
Optimizer only takes one inputs
\end_layout

\begin_layout Itemize
obj = @(x) my_func(x,parameters)
\end_layout

\end_deeper
\begin_layout Standard
\begin_inset Separator parbreak
\end_inset


